<%= form_for(@print) do |f| %>
  <% if @print.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@print.errors.count, "error") %> prohibited this post from being saved:</h2>

      <ul>
      <% @print.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <%= f.file_field :file %>
  <%= f.select(:printer, options_for_select([['HUBBEN', 'ed-2338-laser1']])) %>
  <%= f.label(:copies, "Number of copies") %>
  <%= f.number_field(:copies) %>
  <%= f.label(:duplex, "Doublepaged: ") %>
  <%= f.check_box(:duplex, checked: true) %>
  <%= f.label(:ranges, "Range: ") %>
  <%= f.text_field(:ranges) %>
  <%= f.label(:media, "Papersize:") %>
  <%= f.select(:media, options_for_select([['A4', 'A4'],['A3', 'A3']])) %>
  <%= f.label(:ppi, "Resolution:") %>
  <%= f.select(:ppi, options_for_select([['Auto', 'auto'],['600dpi', '600'],['1200dpi', '1200']])) %>
  <%= f.label(:username, "Username:") %>
  <%= f.text_field(:username) %>
  <%= f.label(:password, "Password:") %>
  <%= f.password_field(:password) %>
  <%= submit_tag %>
<% end %>