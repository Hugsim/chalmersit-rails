<p id="notice"><%= notice %></p>

<h1>Listing User Group Infos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_group_infos.each do |user_group_info| %>
      <tr>
        <td><%= user_group_info.user.id %></td>
        <td><%= user_group_info.group.slug %></td>
        <td><%= link_to t('show'), user_group_info %></td>
        <td><%= link_to t('edit'), edit_user_group_info_path(user_group_info) %></td>
        <td><%= link_to t('delete'), user_group_info, method: :delete, data: { confirm: t('confirm') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('new', name: UserGroupInfo.model_name.human.downcase), new_user_group_info_path %>
